{
  "env": {
    "es6": true,
    "mocha": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  },
  "level 0": {
    "level 1": {
      "level 2": {
        "level 3": {
          "level 4": {
            "level 5": {
              "level 6": {
                "level 7": {
                  "level 8" : "value"
              }
            }
          }
        }
      }
    }
  }
}
}